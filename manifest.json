{
    "manifest_version": 3,
    "name": "JSON Pretty Print",
    "version": "1.0",
    "description": "Formats JSON data on webpages into a readable, pretty-printed format with syntax highlighting.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["content.css"]
      }
    ],
    "icons": {
      "48": "./icons/icon48.png",
      "128": "./icons/icon128.png"
    }
  }